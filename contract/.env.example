# Environment variables for CSA deployment

# Deployment wallet private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Cronos RPC URLs
CRONOS_TESTNET_RPC=https://evm-t3.cronos.org
CRONOS_MAINNET_RPC=https://evm.cronos.org

# Deployed contract addresses (filled after deployment)
SENTINEL_CLAMP_ADDRESS=

# Agent configuration
AGENT_WALLET_ADDRESS=

# Optional: Block explorer API key for verification
CRONOSCAN_API_KEY=
